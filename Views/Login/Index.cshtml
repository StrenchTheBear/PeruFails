@model login

<h1>Login</h1>

<form action="" method="post">

    <div asp-validation-summary="All"></div>

    <div class="form-group">
        <label asp-for="Correo"></label>
        <input asp-for="Correo" class="form-control" required>
    </div>
    <div class="form-group">
        <label asp-for="Contraseña"></label>
        <input asp-for="Contraseña" class="form-control" required>
    </div>
    
    <button  class="btn btn-dark">Registrarse</button>
</form>
